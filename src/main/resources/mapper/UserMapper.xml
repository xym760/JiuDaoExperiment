<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jiudao.experiment.mapper.UserMapper">
    <select id="getUserByUsername" parameterType="string" resultType="com.jiudao.experiment.entity.UserEntity">
        select * from user where username = #{username}
    </select>
</mapper>

